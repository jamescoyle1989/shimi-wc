    "@storybook/web-components-vite": "^7.6.8",
    "@types/chroma-js": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.15",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
