{
  "name": "shimi-wc",
  "version": "1.5.2",
  "description": "A collection of web components for building complex MIDI applications",
  "type": "module",
  "main": "dist/index.mjs",
  "unpkg": "dist/index.mjs",
  "repository": {
    "type": "git",
